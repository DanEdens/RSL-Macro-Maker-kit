                    <Action Name="Variable Editor" Enabled="False" XML_Guid="{554DC104-2489-47AC-BA38-838F7EDE2123}">
                        EventGhost.PythonScript(u'egg = eg.globals\negp = eg.plugins\n\n\nvar = eg.event.payload\n# var = temp.split(\':\')\n\nsetattr(eg.globals, var[0], var[1])\n\nprint \'eg.globals.\' + var[0] + \' set to: \'+ var[1]\n# except: \n    #print \'Error\'\nx = """\ntry:\n    tmp = eg.event.suffix\n    print tmp\n    temp = tmp.split()\n    data = temp[1]\nexcept IndexError:\n    try:\n        data = eg.event.payload[0]\n    except TypeError:\n        data = eg.event.suffix[-1:]\n"""')
                    </Action>
                    <Action Name="Variables set:" XML_Guid="{554DC104-2489-47AC-BA38-838F7EDE2123}">
                        EventGhost.PythonScript(u'egg = eg.globals\negp = eg.plugins\n\n\ntemp = eg.event.suffix\nvar = temp.split(\'=:=\')\n\ntry:\n    setattr(eg.globals, var[1], eg.event.payload)\n    print \'eg.globals.\' + var[1] + \' set to: \'+ eg.event.payload\nexcept:\n    print \'Error\'\nx = """\ntry:\n    tmp = eg.event.suffix\n    print tmp\n    temp = tmp.split()\n    data = temp[1]\nexcept IndexError:\n    try:\n        data = eg.event.payload[0]\n    except TypeError:\n        data = eg.event.suffix[-1:]\n"""')
                    </Action>
                </Macro>
