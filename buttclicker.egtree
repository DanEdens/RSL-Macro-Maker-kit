                    <Macro Name="email crawler" XML_Guid="{C12C0857-4C06-4E69-A888-89F1E9606D3C}">
                        <Action Name="email crawler" XML_Guid="{7EB7D69F-DD1C-460E-ACCC-17D544C4E1D8}">
                            EventGhost.PythonScript(u'import requests\nimport re\n\n# get url\nurl = input(\'Enter a URL (include `http://`): \')\n\n# connect to the url\nwebsite = requests.get(url)\n\n# read html\nhtml = website.text\n\n# use re.findall to grab all the links\nlinks = re.findall(\'"((http|ftp)s?://.*?)"\', html)\nemails = re.findall(\'([\\w\\.,]+@[\\w\\.,]+\\.\\w+)\', html)\n\n\n# print the number of links in the list\nprint("\\nFound {} links".format(len(links)))\nfor email in emails:\n    print(email)')
                        </Action>
