                        <Action Name="Move and Click" XML_Guid="{DAE8C2FD-375C-4F82-9048-BBECBD592E5C}">
                            EventGhost.PythonScript(u"import win32api\nfrom random import randint\n\neg.globals.raidscreenshot = False\n\ndef click(_x, _y):\n    randx = randint(-2, 2)\n    randy = randint(-2, 2)\n    eg.globals.h_x = _x + randx\n    eg.globals.h_y = _y + randy\n    print eg.globals.h_x, eg.globals.h_y\n    eg.globals.xtemp, eg.globals.ytemp = win32api.GetCursorPos()\n    eg.plugins.Mouse.MoveAbsolute(eg.globals.h_x, eg.globals.h_y, eg.globals.foomonitor, False, True)\n    eg.plugins.EventGhost.Wait(0.2)\n    eg.plugins.Mouse.LeftDoubleClick()\n    eg.plugins.EventGhost.Wait(0.1)\n\n\ntry:\n    tmp = eg.event.suffix\n    temp = tmp.split()\n    pos = temp[1]\nexcept IndexError:\n    try:\n        pos = eg.event.payload[0]\n    except:\n        pos = 5\n\npos = str(pos)\nprint 'Coordinate Identifier: '+pos\n\nif eg.globals.raidscreenshot:\n    eg.plugins.Window.SendKeys(u'{Shift+Alt+P}', False, 2)\n\n\ntry:\n    _x = getattr(eg.globals, 'x'+pos)\n    _y = getattr(eg.globals, 'y'+pos)\n    click(_x, _y)\nexcept AttributeError:\n    print 'Coords not found. Default to rebattle..'\n    eg.plugins.EventGhost.TriggerEvent(u'raid_rebattle', 0.0, None, False, False, False)\n\n\n\n")
                        </Action>
                    </Macro>
